<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lSQL">
		<sql id="selCNT">
			SELECT
				count(*) cnt
			FROM
				member
			WHERE
				memid = 
		
		</sql>
		
		<select id="Login" resultType="int" parameterType="lVO">
			
		<include refid="selCNT" /> #{id}
			AND mempw = #{pw}
		</select>
		<select id="idCount" resultType="int" parameterType="string">
			<include refid="selCNT" />#{id}
		</select>	
		
		<select id="kidCount" resultType="int" parameterType="int">
			SELECT
				count(*) cnt
			FROM
				kmember
			WHERE
				kid = #{kid}
		</select>
		
		<insert id="join">
			INSERT INTO
				member(memno, memid, mempw, phone, name, membir, carno)
		
		</insert>
		
		
</mapper>